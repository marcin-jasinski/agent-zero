# Agent Zero - Grafana Dashboard Configuration
# This dashboard provides comprehensive observability for Agent Zero

{
  "dashboard": {
    "id": null,
    "uid": "agent-zero-main",
    "title": "Agent Zero - Observability Dashboard",
    "tags": ["agent-zero", "rag", "llm"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate (per minute)",
        "targets": [
          {
            "expr": "rate(agent_zero_requests_total[1m])",
            "legendFormat": "{{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Latency (p95, p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_zero_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(agent_zero_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{endpoint}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "graph",
        "title": "Document Retrieval Count",
        "targets": [
          {
            "expr": "rate(agent_zero_retrieval_documents_count_sum[5m]) / rate(agent_zero_retrieval_documents_count_count[5m])",
            "legendFormat": "{{retrieval_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Documents"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "graph",
        "title": "Retrieval Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_zero_retrieval_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{retrieval_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "graph",
        "title": "Embedding Generation Time",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_zero_embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(agent_zero_embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "LLM Token Usage",
        "targets": [
          {
            "expr": "rate(agent_zero_llm_tokens_total{type='input'}[5m])",
            "legendFormat": "Input - {{model}}",
            "refId": "A"
          },
          {
            "expr": "rate(agent_zero_llm_tokens_total{type='output'}[5m])",
            "legendFormat": "Output - {{model}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Tokens/sec"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "LLM Generation Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_zero_llm_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 8,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "type": "stat",
        "title": "Total Requests (24h)",
        "targets": [
          {
            "expr": "sum(increase(agent_zero_requests_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      
      {
        "id": 9,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "type": "stat",
        "title": "Error Rate (5m)",
        "targets": [
          {
            "expr": "sum(rate(agent_zero_requests_total{status='error'}[5m])) / sum(rate(agent_zero_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "unit": "percent"
        }
      },
      
      {
        "id": 10,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "type": "stat",
        "title": "Active Requests",
        "targets": [
          {
            "expr": "sum(agent_zero_active_requests)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value"
        }
      },
      
      {
        "id": 11,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "type": "stat",
        "title": "Qdrant Collection Size",
        "targets": [
          {
            "expr": "agent_zero_qdrant_collection_size",
            "legendFormat": "{{collection}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value"
        }
      },
      
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "type": "graph",
        "title": "Document Ingestion Rate",
        "targets": [
          {
            "expr": "rate(agent_zero_documents_ingested_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Documents/sec"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "type": "graph",
        "title": "LLM Guard Scans",
        "targets": [
          {
            "expr": "rate(agent_zero_llm_guard_scans_total[5m])",
            "legendFormat": "{{type}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Scans/sec"},
          {"format": "short", "show": false}
        ]
      }
    ]
  }
}
